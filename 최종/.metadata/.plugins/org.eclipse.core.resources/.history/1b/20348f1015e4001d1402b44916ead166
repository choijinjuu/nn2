package com.semi.order.model.vo;

import java.sql.Date;

public class Payment {
	private int paymentNumber;//PAYMENT_NUMBER NUMBER
	private int orderNo;//ORDER_NO NUMBER
	private int memberNo;//MEMBER_NO NUMBER
	private String productNo;//PRODUCT_NO NUMBER
	private Date createdAt;//CREATED_AT DATE
	private int payment;//PAYMENT NUMBER
	private String orderRequest;//ORDER_REQUEST VARCHAR2(600 BYTE)
	private String bankName;//BANK_NAME VARCHAR2(20 BYTE)
	private String depositName;//DEPOSIT_NAME VARCHAR2(20 BYTE)
	private int usePoint;//USE_POINT NUMBER
	private String addressName;//ADDRESS_NAME VARCHAR2(20 BYTE)
	private String post;//POST VARCHAR2(20 BYTE)
	private String roadAddress;//ROAD_ADDRESS VARCHAR2(100 BYTE)
	private String detailAddress;//DETAIL_ADDRESS VARCHAR2(100 BYTE)
	private String state;//STATE VARCHAR2(30 BYTE)
	private int deliveryCost;//DELIVERY_COST NUMBER
	
	
}
